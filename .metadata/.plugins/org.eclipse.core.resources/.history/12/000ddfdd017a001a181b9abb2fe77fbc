class Employee{
	String name;
	int age;
	void gotWork() {
		System.out.println(name +","+age+"인 사람이 일을한다.");
	}
}
public class Test2 {
	public static void main(String[] args) {
		//value-type(값타임)vsreference-type(참조타입)
		//jvm
		//stack,heap
		
		int a =1;
		int b = a;
		
		Employee emp = new Employee();

		emp.gotWork();
		
	}
}
